{
  "name": "turbo-template",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "types:check": "turbo run types:check",
    "checks:fix": "prettier --write \"**/*.{ts,tsx,md,css}\"",
    "checks": "prettier --check \"**/*.{ts,tsx,md,css}\" && turbo run types:check lint",
    "db:check": "turbo run db:check",
    "db:migrate": "turbo run db:migrate",
    "db:studio": "turbo run db:studio",
    "db:generate": "turbo run db:generate",
    "db:push": "turbo run db:push",
    "db:start": "docker compose up -d",
    "db:stop": "docker compose down -v",
    "auth:generate": "turbo run auth:generate",
    "ui:add": "turbo run ui:add",
    "prepare": "husky",
    "env:pull": "vercel env pull .env",
    "sync": "git fetch template && git merge template/main",
    "sync:initial": "git fetch template && git merge template/main --allow-unrelated-histories",
    "setup:sync": "git remote get-url template >/dev/null 2>&1 && git remote set-url template https://github.com/impulse-studio/ultimate-template.git || git remote add template https://github.com/impulse-studio/ultimate-template.git"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "turbo": "^2.6.0",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
